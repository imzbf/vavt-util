{
  "name": "@vavt/util",
  "version": "1.1.1",
  "license": "MIT",
  "keywords": [
    "js",
    "util"
  ],
  "description": "JavaScript library.",
  "scripts": {
    "dev": "tsx ./scripts/dev",
    "build": "tsx ./scripts/build.lib",
    "build:es": "tsx ./scripts/build.lib es",
    "build:cjs": "tsx ./scripts/build.lib cjs type",
    "build:umd": "tsx ./scripts/build.lib umd",
    "lint": "eslint --ext .ts ./",
    "lint:fix": "eslint --fix --ext .ts ./",
    "prepare": "husky install"
  },
  "homepage": "https://imzbf.github.io/vavt-util/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imzbf/vavt-util.git"
  },
  "bugs": {
    "url": "https://github.com/imzbf/vavt-util/issues"
  },
  "files": [
    "lib"
  ],
  "types": "./lib/types/index.d.ts",
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/es/index.mjs",
  "devDependencies": {
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.0.2",
    "vite": "^4.3.8",
    "vite-plugin-dts": "^2.3.0"
  },
  "lint-staged": {
    "**/*.{ts,md}": [
      "prettier --write"
    ],
    "**/*.ts": [
      "eslint --quiet --fix"
    ]
  }
}
